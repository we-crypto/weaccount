{
  "name": "@wecrpto/weaccount",
  "version": "0.2.4",
  "description": "a crypto lib for miniprogram",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "scripts": {
    "prettier": "prettier src/*.{js,ts,html} --ignore-path .prettierignore",
    "lint": "eslint --fix src/*.ts",
    "types": "tsc --noEmit",
    "build": "yarn build:clean & yarn build:lib",
    "build:clean": "rimraf ./dist",
    "build:lib": "rollup --config ci/rollup.lib.js --noConflict",
    "build:dev": "rollup --config ci/rollup.dev.js -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/we-crypto/weaccount.git"
  },
  "keywords": [
    "ed25519",
    "cryptojs",
    "miniprogram",
    "signature",
    "verify"
  ],
  "private": false,
  "files": [
    "dist",
    "types"
  ],
  "author": "lanbery <lanbery@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/we-crypto/weaccount/issues"
  },
  "homepage": "https://github.com/we-crypto/weaccount#readme",
  "dependencies": {
    "@wecrpto/nacl": "^0.1.0",
    "@wecrpto/nacl-util": "^0.1.1",
    "buffer": "^6.0.3",
    "crypto-js": "3.1.9-1",
    "js-base64": "^3.6.0",
    "scrypt-js": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.12.17",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@types/crypto-js": "3.1.43",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "chalk": "^4.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jsdoc": "^32.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.5"
  }
}
